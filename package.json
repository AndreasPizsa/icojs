{
  "name": "icojs",
  "description": "parse ico file",
  "version": "0.3.0",
  "author": "egy186",
  "bugs": {
    "url": "https://github.com/egy186/icojs/issues"
  },
  "dependencies": {
    "canvas": "^1.2.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^3.0.0",
    "eslint": "^1.10.3",
    "eslint-config-eslint": "^1.0.1",
    "gulp": "^3.9.0",
    "gulp-mustache": "^1.0.2",
    "istanbul": "^0.4.1",
    "jsdoc": "^3.3.2",
    "minifyify": "^7.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4"
  },
  "files": [
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "homepage": "https://egy186.github.io/icojs",
  "keywords": [
    "ico",
    "parse"
  ],
  "license": "MIT",
  "main": "src/ico.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egy186/icojs.git"
  },
  "scripts": {
    "build": "mkdirp dist && browserify src/ico.js --debug --plugin [ minifyify --map ico.js.map --output dist/ico.js.map ] --transform babelify --exclude canvas --exclude buffer --outfile dist/ico.js",
    "lint": "eslint src test",
    "test": "istanbul cover --root src --report lcov --dir coverage node_modules/mocha/bin/_mocha -- test/*.js"
  }
}
