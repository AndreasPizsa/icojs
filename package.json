{
  "name": "icojs",
  "description": "parse ico file",
  "version": "0.4.0",
  "author": "egy186",
  "bugs": {
    "url": "https://github.com/egy186/icojs/issues"
  },
  "dependencies": {
    "jimp": "^0.2.21"
  },
  "devDependencies": {
    "@egy186/eslint-config": "^0.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "eslint": "^2.13.0",
    "istanbul": "^0.4.2",
    "jsdoc-to-markdown": "^1.3.3",
    "minifyify": "^7.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "rimraf": "^2.5.0"
  },
  "engines": {
    "node": ">=4.2.3"
  },
  "files": [
    "src",
    "LICENSE",
    "README.md",
    "browser.js",
    "index.js",
    "package.json"
  ],
  "homepage": "https://egy186.github.io/icojs",
  "keywords": [
    "ico",
    "parse"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egy186/icojs.git"
  },
  "scripts": {
    "build": "mkdirp dist && browserify src/browser/index.js --debug --plugin [ minifyify --map ico.js.map --output dist/ico.js.map ] --transform babelify --standalone ICO --outfile dist/ico.js",
    "clean": "rimraf coverage dist",
    "doc": "jsdoc2md -f src/ico.js -t templates/README.md > README.md",
    "lint": "eslint src test",
    "test": "istanbul cover --root src --report lcov --dir coverage node_modules/mocha/bin/_mocha -- test/*.js"
  }
}
