{
  "env": {
    "browser": true,
    "es6": false
  },
  "globals": {
    "$": true,
    "ICO": true,
    "hljs": true
  },
  "rules": {
    "func-names": "off",
    "no-console": "off",
    "no-var": "off",
    "prefer-arrow-callback": "off",
    "prefer-template": "off",
    "strict": "off",
    "vars-on-top": "off"
  }
}
